local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("evos", "DarkTheme")

-- MAIN
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Main")


MainSection:NewButton("Doors script", "doors script", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/final22132/script/main/doors%20script?token=GHSAT0AAAAAACBHGSRVTNVHJ46C7RPLKG7WZBUG2JQ'))()
end)


MainSection:NewButton("Breaking points script", "Breaking points script", function()
   loadstring(game:HttpGet('https://raw.githubusercontent.com/final22132/script/main/breaking%20points%20script?token=GHSAT0AAAAAACBHGSRVG6ZQTIGE44OKF24YZBUHK6Q'))()
end)

MainSection:NewButton("Flag wars script", "Flag wars script", function()
   loadstring(game:HttpGet('https://raw.githubusercontent.com/final22132/script/main/flag%20wars%20script?token=GHSAT0AAAAAACBHGSRUUJB6HP6E3KYGNV74ZBUHS4Q'))()
end)

MainSection:NewButton("Funky friday script", "Funky friday script", function()
   local vu = game:GetService("VirtualUser")
game:GetService("Players").LocalPlayer.Idled:connect(function()
   vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
   wait(1)
   vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
end)
loadstring(game:HttpGet("https://raw.githubusercontent.com/wally-rblx/funky-friday-autoplay/main/main.lua",true))()
 local message = Instance.new("Message", workspace)
        message.Text = "Loaded! If the script is not working, press 'F9' to check for any errors."
        wait(4.5)
        message:Destroy()
end)

MainSection:NewButton("Epic minigames script", "Epic minigames script", function()
   loadstring(game:HttpGet("https://raw.githubusercontent.com/SlamminPig/rblxgames/main/Epic%20Minigames/EpicMinigamesGUI"))()
end)

MainSection:NewButton("Base Battles script", "Base Battles script", function()
    loadstring(game:HttpGet(("https://pastebin.com/raw/hQ8cDCaM"),true))()
end)

MainSection:NewButton("inf yield ", "inf yield", function()
   loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end)

MainSection:NewButton("Build a boat for treasure", " build a boat for treasure", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/XRoLLu/UWU/main/BUILD%20A%20BOAT%20FOR%20TREASURE.lua'))()
end)

MainSection:NewButton("Bedwars", " Bedwars" , function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/7GrandDadPGN/VapeV4ForRoblox/main/NewMainScript.lua", true))()
end)
